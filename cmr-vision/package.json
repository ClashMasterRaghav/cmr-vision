{
  "name": "cmr-vision",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://clashmasterraghav.github.io/cmr-vision/",
  "main": "public/electron.js",
  "dependencies": {
    "@react-three/drei": "9.88.17",
    "@react-three/fiber": "8.15.11",
    "cra-template-typescript": "1.3.0",
    "electron": "35.1.4",
    "electron-is-dev": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "three": "0.160.0",
    "web-vitals": "4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "echo \"Tests skipped\" && exit 0",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "release": "npm run build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/three": "0.158.2",
    "concurrently": "7.6.0",
    "electron-builder": "24.13.3",
    "jest": "27.5.1",
    "typescript": "5.2.2",
    "wait-on": "7.2.0"
  },
  "build": {
    "appId": "com.cmr.vision",
    "productName": "CMR Vision",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.entertainment",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Entertainment"
    }
  }
}
